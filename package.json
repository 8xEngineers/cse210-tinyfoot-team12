{
  "name": "littlefoot",
  "version": "1.0.0-alpha.13",
  "description": "Footnotes without the footprint.",
  "repository": "goblindegook/littlefoot",
  "scripts": {
    "clean": "rm -rf coverage dist",
    "build": "mkdir -p dist && npm run build-scripts && npm run build-styles",
    "build-scripts": "cd src; babel *.js **/*.js -d ../dist",
    "build-styles": "node-sass --include-path styles -o dist/ styles/",
    "docs": "esdoc -c esdoc.json",
    "lint": "eslint {src,test}/**/*.js",
    "prepublish": "npm run build",
    "pretest": "npm run lint && npm run build-styles",
    "test": "karma start --single-run",
    "test-local": "karma start --single-run --browsers ChromeCanary",
    "watch": "karma start --browsers ChromeCanary"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "author": {
    "name": "Lu√≠s Rodrigues",
    "email": "hello@goblindegook.net",
    "url": "https://goblindegook.com/"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "style": "styles/css/littlefoot.css",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-istanbul": "^0.8.0",
    "babel-plugin-object-assign": "^1.2.1",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "browserify-istanbul": "^2.0.0",
    "esdoc": "^0.4.3",
    "eslint": "^2.5.3",
    "istanbul": "^0.4.2",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-tap": "^1.0.4",
    "node-sass": "^3.7.0",
    "simulant": "^0.2.1",
    "sinon": "^1.17.3",
    "stringify": "^5.1.0",
    "tape": "^4.4.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "delegate": "^3.0.1",
    "dom-classlist": "^1.0.1",
    "dom-closest": "^0.2.0",
    "dom-matches": "^2.0.0",
    "dom-siblings": "^0.1.0",
    "lodash": "^4.5.1"
  }
}

{
  "name": "littlefoot",
  "version": "1.0.0-alpha.6",
  "description": "Footnotes without the footprint.",
  "repository": "goblindegook/littlefoot",
  "scripts": {
    "build": "npm run build-scripts && npm run build-styles",
    "build-scripts": "cd src; babel *.js **/*.js -d ../lib",
    "build-styles": "node-sass --include-path styles/sass -o styles/css/ styles/sass/",
    "docs": "esdoc -c esdoc.json",
    "prepublish": "npm run build",
    "pretest": "eslint src/**/*.js",
    "test": "karma start --single-run",
    "watch": "karma start --browsers PhantomJS"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "author": {
    "name": "Lu√≠s Rodrigues",
    "email": "hello@goblindegook.net",
    "url": "https://goblindegook.com/"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "style": "styles/css/littlefoot.css",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-istanbul": "^0.7.0",
    "babel-plugin-object-assign": "^1.2.1",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-istanbul": "^2.0.0",
    "es6-promise": "^3.1.2",
    "esdoc": "^0.4.3",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.2",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.4",
    "karma-tap": "^1.0.4",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.7",
    "simulant": "^0.2.0",
    "sinon": "^1.17.3",
    "stringify": "^5.1.0",
    "tape": "^4.4.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "dom-classlist": "^1.0.1",
    "dom-closest": "^0.2.0",
    "dom-matches": "^2.0.0",
    "dom-siblings": "^0.1.0",
    "lodash": "^4.5.1"
  }
}
